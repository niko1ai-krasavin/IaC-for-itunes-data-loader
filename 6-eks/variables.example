# Name of ssh-key for access to nodes
variable "ssh_key_name_for_eks_cluster" {
  default = ""
  type    = string
}

# EKS Cluster name
variable "cluster_name" {
  default = ""
  type    = string
}

# Variables ID of VPC and subnets
variable "id_of_custom_vpc" {
  description = "ID of custom vpc"
  type        = string
  default     = ""
}

variable "id_of_eks_pub_subnet_az_a" {
  description = "ID of public subnet in AZ-a"
  type        = string
  default     = ""
}

variable "id_of_eks_pub_subnet_az_b" {
  description = "ID of public subnet in AZ-b"
  type        = string
  default     = ""
}
